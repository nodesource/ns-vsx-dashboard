{
  "name": "ns-vsx-dashboard",
  "displayName": "ns-vsx-dashboard",
  "description": "Provides a dashboard of your running N|Solid processes right in Visual Studio Code.",
  "version": "0.0.1",
  "publisher": "nodesource",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension",
  "activationEvents": [
    "*"
  ],
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run tsc",
    "watch": "tsc -watch -p ./",
    "tsc": "rm -rf out/* && tsc -p .",
    "tsl": "tslint -p . --force -t stylish",
    "tsl:fix": "tslint -p . --force -t stylish --fix",
    "tsfmt": "tsfmt --baseDir . -r",
    "lint": "npm run tsc &&  npm run tsl",
    "post:pretty-bytes": "cd node_modules/@types && ln -sf pretty-bytes thlorenz__pretty-bytes",
    "post:vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "npm run post:vscode && npm run post:pretty-bytes",
    "test": "node ./node_modules/vscode/bin/test",
    "webview.list": "(cd web/list && npm run webview)"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "~10.12.10",
    "@types/pretty-bytes": "~5.1.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.1",
    "typescript-formatter": "~7.2.2",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "@thlorenz/pretty-bytes": "~5.1.1",
    "flamegraph": "~1.3.0",
    "time-ago": "~0.2.1",
    "toolkit-zmq": "0.0.0"
  }
}
